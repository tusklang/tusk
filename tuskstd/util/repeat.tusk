var repeat

ovld repeat = fn(string -> x, int -> _n) {
    fin := "" ;final return var
    n := clone:(_n) ;clone _n
    while (n > 0) { ;keep going until n is 0
        fin+=x ;concat the adder to fin
        n-- ;decrement
    }
    return fin ;return the final return var
}

ovld repeat = fn(array -> x, int -> _n) {
    fin := []
    n := clone:(_n)

    while (n > 0) {

        each (x, _, v)
            fin = append:(fin, v)

        n--
    }

    return fin
}